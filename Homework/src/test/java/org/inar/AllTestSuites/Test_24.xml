<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <listeners>
        <listener class-name="org.inar.utilities.Listeners"/>
    </listeners>
    <test verbose="2" preserve-order="true"
          name="Test Case 24: Download Invoice after purchase order">
        <classes>
            <class name="org.inar.automationexercise.TestCase_24">
                <methods>
                    <include name="verifyHomePageIsVisible"/>
                    <include name="verifyThatCartPageIsDisplayed"/>
                    <include name="verifyAccountCreatedAndClickContinueButton"/>
                    <include name="verifyLoggedInAsUsernameAtTop"/>
                    <include name="verifyAddressDetailsAndReviewYourOrder"/>
                    <include name="verifySuccessMessageOurOrderHasBeenPlacedSuccessfully"/>
                    <include name="clickDownloadInvoiceButtonAndVerifyInvoiceIsDownloadedSuccessfully"/>
                    <include name="verifyACCOUNTDELETEDAndClickContinueButton"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>