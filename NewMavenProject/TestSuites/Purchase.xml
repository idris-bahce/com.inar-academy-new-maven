<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="All Test Suite">

    <groups>
        <run>
            <include name="Purchase"/>
        </run>
    </groups>

    <test thread-count="5" verbose="2" preserve-order="true"
          name="Submit Order Test">
        <classes>
            <class name="org.inaracademy.SubmitOrderTest">
            </class>
        </classes>
    </test>
    <test thread-count="5" verbose="2" preserve-order="true"
          name="Error Validation Test">
        <classes>
            <class name="org.inaracademy.ErrorValidations">
            </class>
        </classes>
    </test>
</suite>